{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' next dev",
    "dev:https": "NODE_OPTIONS='--no-deprecation' next dev --experimental-https",
    "build": "NODE_OPTIONS='--no-deprecation' next build",
    "start": "NODE_OPTIONS='--no-deprecation' next start",
    "pages:build": "npm run build && npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "lint": "next lint",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --config .prettierrc --ignore-path .prettierignore --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@uiw/react-md-editor": "^3.6.0",
    "axios": "^1.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dnd-core": "^16.0.1",
    "lucide-react": "^0.511.0",
    "next": "15.5.2",
    "next-remove-imports": "^1.0.12",
    "prismjs": "^1.30.0",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.65.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "overrides": {
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.14",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.4.23",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wrangler": "^4.45.0"
  }
}
